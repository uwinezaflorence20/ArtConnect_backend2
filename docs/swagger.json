{
    "swagger": "2.0",
    "info": {
      "title": "ArtConnect",
      "description": "ArtConnect  API Documentation",
      "version": "1.0.0"
    },
    "host": "localhost:3000",
    "basePath": "",
    "schemes": ["http", "https"],
    "paths": {
      "/signUp": {
        "post": {
          "summary": "creating new user account ",
          "description": "signing up for new user of ArtConnect",
          "tags": ["User"],
          "parameters": [
            {
              "in": "body",
              "name": "body",
              "schema": {
                "$ref": "#/definitions/User"
              },
              "required": true,
              "description": "User Signup in ArtConnect"
            }
          ],
          "responses": {
            "201": {
              "description": "user created successfully"
            }
          }
        }
      }
     
    },
    "definitions": {
      "User": {
        "type": "object",
        "properties": {
          "FirstName": {
            "type": "string"
          },
          "LastName": {
            "type": "string"
          },
          "Email": {
            "type": "string"
          },
          "Password": {
            "type":"string"
          },
          "ConfirmPassword": {
            "type":"string"
          },
          "Otp":{
            "type":"number"
          },
          "OtpVerified":{
            "type":"boolean",
            "default":"false"
          }

        },
        "required": ["FirstName","LastName","Email","Password","ConfirmPassword"]
      }
}
}